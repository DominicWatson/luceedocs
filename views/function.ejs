<style>
	.syntaxAttr={color:maroon;}
</style>
<div class="jumbotron">
	<h1>cf<%= func.name %> </h1>
	<p><%= func.description %></p>
	<p>Returns: <%= func.returnType %></p>
	<%
		String.prototype.capitalize = function() {
    		return this.charAt(0).toUpperCase() + this.slice(1);
		}
		var labeltype = 'label-default';
		var statusLabel = func.status.capitalize();
		if(func.status == 'implemented' || func.status == 'implemeted'){
			labeltype = 'label-success';
			statusLabel = 'Implemented';
		}
		if(func.status == 'deprecated'){
			labeltype = 'label-warning';
		
		}

		
	%>
	<h3><span class="label <%= labeltype %>"><%= statusLabel %></span></h3>
	
</div>


<section id="fun_description">
 	<hr>
	<% if(func.status=='deprecated'){ %>
	<div class="alert alert-info">
		 <strong>Warning!</strong> This function has been depricated
	</div>
	<% } %>
	<div class="well">
	Type: <span class="icon-java-bold"></span> <%= func.type %> 
	</div>
	
	<h4>Example</h4>
	
	<pre class="prettyprint"><%= tagcode %></pre>
	<pre class="prettyprint"><%= scriptcode %></pre>


	<% if(Object.keys(func.member).length){ %>
	<h4>Member functionality</h4>
		<table class="table table-bordered">
			<tbody>
			<tr><th>Chanining Allowed?</th><td><%= func.member.chaining %></td></tr>
			<% if(func.member.name.length){%>

				<tr><th>Member name:</th><td>
					<a href="/object/<%= func.member.type + '/' + func.name %>">
					<em><%= func.member.type.capitalize() %></em>.<strong><%= func.name %></strong>( <%= argumentcode %> )
						</a>

				</td></tr>
			<% } %>
			</tbody>
	</table>
	<% } %>

	<h4>Arguments</h4>
	<cfif data.getargumentType() EQ "fixed" and not arraylen(data.getarguments())>
	<div class="text">#stText.doc.arg.zero#</div>
	<cfelseif Len(data.getargumentType())>
		<div class="text">

			#stText.doc.arg.type[data.getargumentType()]#

			<cfif data.getargumentType() EQ "dynamic">
			<cfif data.getargMin() GT 0 and data.getargMax() GT 0>
				#replace(replace(stText.doc.arg.minMax,"{min}",data.getargMin()),"{max}",data.getargMax())#
				<cfelseif data.getargMin() GT 0>
					#replace(stText.doc.arg.min,"{min}",data.getargMin())#
					<cfelseif data.getargMax() GT 0>
						#replace(stText.doc.arg.max,"{max}",data.getargMax())#
			</cfif>
			</cfif>
		</div>

		</cfif>
		<cfif data.getargumentType() EQ "fixed" and arraylen(data.getarguments())>
		<table class="table table-striped table table-bordered">
			<thead>
			<tr>
				<th width="21%">#stText.doc.arg.name#</th>
				<th width="7%">#stText.doc.arg._type#</th>
				<th width="7%">#stText.doc.arg.required#</th>
				<th width="65%">#stText.doc.arg.description#</th>
			</tr>
			</thead>
			<tbody>
			<cfloop array="#data.getarguments()#" index="attr">
				<cfif attr.status EQ "hidden"><cfcontinue></cfif>
					<tr>
						<td>#attr.name	#</td>
						<td>#attr.type#&nbsp;</td>
						<td>#YesNoFormat(attr.required)#</td>
						<td><cfif attr.status EQ "deprecated"><b class="error">#stText.doc.depArg#</b><cfelse>#formatDesc(attr.description)#</cfif>&nbsp;</td>
					</tr>
			</cfloop>
			</tbody>
		</table>
		</cfif>

	<pre><%=  JSON.stringify(func,null,4) %></pre>
